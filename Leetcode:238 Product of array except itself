class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n=nums.size();
        vector <int> v;
        for(int i=0;i<n;i++){
            int pr=1;
            for(int j=0;j<n;j++){
                if(i==j) continue;
                else{
                    pr=pr*nums[j];
                    
                }
            }
            v.push_back(pr);
        }
        return v;
    }
};
